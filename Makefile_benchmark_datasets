BENCHMARK_HARRIS_SIFT = benchmark_harrisSift
CALCULATE_SRR = calculate_srr

CVLOG_FILTER = demo_cvlog

cvlog:
	$(CC) -o $(BIN_DIR)/$(CVLOG_FILTER) $(TEST_DIR)/$(CVLOG_FILTER).cpp $(SRC_FILES) $(CV_LIB)

benchmark_sift:
	$(CC) -o $(BIN_DIR)/$(BENCHMARK_SIFT) $(TEST_DIR)/$(BENCHMARK_SIFT).cpp $(SRC_FILES) $(CV_LIB)

benchmark_harris:
	$(CC) -o $(BIN_DIR)/$(BENCHMARK_HARRIS) $(TEST_DIR)/$(BENCHMARK_HARRIS).cpp $(SRC_FILES) $(CV_LIB)

benchmark_harris_sift:
	$(CC) -o $(BIN_DIR)/$(BENCHMARK_HARRIS_SIFT) $(TEST_DIR)/$(BENCHMARK_HARRIS_SIFT).cpp $(SRC_FILES) $(CV_LIB)

calculate_srr:
	$(CC) -o $(BIN_DIR)/$(CALCULATE_SRR) $(TEST_DIR)/$(CALCULATE_SRR).cpp $(SRC_FILES) $(CV_LIB)

