	./$(BIN_DIR)/$(BENCHMARK_HARRIS) $(PROJECTROOM)/scene-0/scene-0.hdr $(PROJECTROOM)/ROIs/scene-0_ROI_0.png $(PROJECTROOM)/ROIs/scene-0_ROI_1.png $(PROJECTROOM)/ROIs/scene-0_ROI_2.png $(OUT_DIR_HARRIS)/HDR/rana_projectroom/
